
<h1><%='Bienvenue'%> <%= current_user.first_name%></h1>

<div>

  <ul>
    <% @my_vehicules = current_user.vehicules %>
    <% @my_vehicules.each do |vehicule| %>
     <li><%= vehicule.name %></li>
  </ul>
    <%= link_to 'Modifier', edit_vehicule_path(vehicule) %>
    <%= link_to 'Supprimer', vehicule_path(vehicule), data: {turbo_method: :delete, turbo_confirm: 'Are you sure?'} %>
    <% end %>
</div>
